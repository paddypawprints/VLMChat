{
  "model": {
    "model_path": "HuggingFaceTB/SmolVLM2-500M-Instruct",
    "max_new_tokens": 1024,
    "eos_token_id": 198,
    "use_onnx": true,
    "onnx_base_path": "~/onnx"
  },
  "conversation": {
    "max_pairs": 10,
    "max_images": 1,
    "history_format": "xml",
    "word_limit": 15
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(levelname)s - %(message)s"
  },
  "paths": {
    "project_root": "None",
    "coco_labels_path": "assets/coco_labels.txt",
    "captured_images_dir": "~/captured_images"
  }
}